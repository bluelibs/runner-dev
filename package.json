{
  "name": "@bluelibs/runner-dev",
  "version": "4.0.0",
  "description": "BlueLibs Runner DevTools",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluelibs/runner-dev"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "start": "node dist/main.js",
    "dev": "npm run build && npm run start",
    "test": "jest --verbose --runInBand",
    "test:dev": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage",
    "test:clean": "jest --clearCache",
    "testonly": "npm test",
    "test:ci": "npm run coverage -- --ci --maxWorkers=2  --reporters=default --reporters=jest-junit",
    "prepublishOnly": "npm run build",
    "typedoc": "typedoc --options typedoc.json",
    "codegen": "graphql-codegen --config codegen.ts",
    "prebuild": "npm run codegen",
    "play": "ts-node src/__tests__/dummy/dummyServer.ts"
  },
  "bin": {
    "runner-dev": "dist/cli.js"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.7",
    "@graphql-codegen/typescript-resolvers": "^4.4.0",
    "@types/benchmark": "^2.1.5",
    "@types/express": "^4.17.21",
    "@types/graphql": "^0.11.3",
    "@types/jest": "^27.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "benchmark": "^2.1.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "express": "^5.1.0",
    "jest": "^29.0.0",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.2.2",
    "source-map-support": "^0.5.13",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.7",
    "typedoc-material-theme": "^1.1.0",
    "typescript": "^5.6.2"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@adobe/jsonschema2md": "^8.0.5",
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "express": "^5.0.0",
    "graphql": "^16.11.0",
    "graphql-voyager": "^1.3.0",
    "lru-cache": "^11.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.23.5"
  },
  "peerDependencies": {
    "@bluelibs/runner": "../runner"
  }
}
